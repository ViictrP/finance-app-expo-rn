{
  "expo": {
    "name": "Finance App",
    "slug": "finance-app-expo-rn",
    "owner": "viictrp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#09090a"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#09090a"
    },
    "android": {
      "backgroundColor": "#09090a",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#09090a"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Finance App built with Expo and React Native"
  }
}
